{
  "name": "Music Eternal - Apply (Hiring Assessment)",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "laravel.test",
  "workspaceFolder": "/var/www/html",
  "customizations": {
    "vscode": {
      "extensions": [
        "bmewburn.vscode-intelephense-client",
        "PKief.material-icon-theme",
        "usernamehw.errorlens",
        "laravel.vscode-laravel",
        "Vue.volar"
      ],
      "settings": {
        "php.debug.executablePath": "/usr/bin/php",
        "php.executablePath": "/usr/bin/php",
        "workbench.colorTheme": "Default Dark+",
        "workbench.iconTheme": "material-icon-theme",
        "editor.formatOnSave": true,
        "[php]": {
          "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
        },
        "[vue]": {
          "editor.defaultFormatter": "Vue.volar"
        },
        "[typescript]": {
          "editor.defaultFormatter": "Vue.volar"
        },
        "pgsql.connections": [
          {
            "id": "1F8F998F-B877-4422-893F-7E46409C1D7C",
            "groupId": "F3003063-B7F8-46B4-A659-0476D131B8F5",
            "authenticationType": "SqlLogin",
            "connectTimeout": 15,
            "applicationName": "vscode-pgsql",
            "clientEncoding": "utf8",
            "sslmode": "prefer",
            "savePassword": true,
            "server": "pgsql",
            "database": "apply",
            "user": "sail",
            "password": "",
            "profileName": "pgsql, apply (sail)",
            "sshPassword": "",
            "expiresOn": 0
          }
        ]
      }
    }
  },
  "remoteUser": "sail",
  "postCreateCommand": "bash .devcontainer/setup.sh",
  "postStartCommand": "bash .devcontainer/start-services.sh",
  "forwardPorts": [
    8000,
    5173
  ],
  "portsAttributes": {
    "8000": {
      "label": "Laravel App",
      "onAutoForward": "notify"
    },
    "5173": {
      "label": "Vite Dev Server",
      "onAutoForward": "silent"
    }
  }
}